{
  "name": "inyourland-rn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev-server": "kyt dev",
    "dev-server-player": "kyt build && node build/server/main.js startScenes",
    "dev-haul": "haul start --platform ios",
    "dev-ios": "react-native run-ios --simulator \"iPad Air\"",
    "test": "jest",
    "test-watch": "jest --watch",
    "haul": "haul",
    "haul-bundle":
      "haul bundle --platform ios --bundle-output ios/build/main.jsbundle --assets-dest build",
    "storybook":
      "storybook start -p 7007 --haul webpack.haul.storybook.js --platform ios",
    "build-lib": "babel src/lib --out-dir build/lib --presets=es2015,stage-3",
    "build-scripts":
      "npm run build-lib && babel src/scripts --out-dir build/scripts --presets=es2015,stage-3",
    "scan": "npm run build-scripts && babel-node build/scripts/scan.js",
    "play": "npm run build-scripts && babel-node build/scripts/play.js",
    "time": "npm run build-scripts && babel-node build/scripts/time.js",
    "content": "npm run build-scripts && babel-node build/scripts/content.js"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "castv2-client": "^1.2.0",
    "chromecast-scanner": "^0.5.0",
    "compression": "1.6.2",
    "cors": "^2.8.4",
    "express": "4.15.3",
    "global": "^4.3.2",
    "kyt": "^0.9.0",
    "lodash": "^4.17.5",
    "mdns": "^2.3.4",
    "react": "^16.3.0-alpha.1",
    "react-dom": "^16.2.0",
    "react-native": "0.54.0",
    "react-native-blur": "^3.2.2",
    "react-native-network-info": "^3.1.0",
    "react-native-svg": "^6.2.2",
    "react-native-svg-loader": "^0.1.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-native": "^4.2.0",
    "react-svg-loader": "^2.1.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-socket.io": "^1.4.0",
    "redux-throttle": "^0.1.1",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.14",
    "@storybook/addon-links": "^3.3.14",
    "@storybook/react-native": "^3.3.14",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "22.4.0",
    "babel-preset-react-native": "4.0.0",
    "csvtojson": "^1.1.9",
    "enzyme": "^3.3.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-kyt": "^0.5.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-react-native": "^3.2.1",
    "haul": "git://github.com/pzhine/haul.git#v1.0.0-beta.13.script-fix",
    "jest": "22.4.0",
    "jest-serializer-enzyme": "^1.0.0",
    "prettier": "^1.11.1",
    "prettier-eslint": "^8.8.1",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-native-mock": "^0.3.1",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-router-native|my-project|react-native-button)/)"
    ]
  },
  "rnpm": {
    "assets": ["./assets/fonts/"]
  }
}
